{
  "name": "graphql-x",
  "version": "0.1.0",
  "description": "GraphQL eXpansion",
  "keywords": [
    "graphql",
    "expansion"
  ],
  "homepage": "https://github.com/tenex-engineering/graphql-x",
  "bugs": {
    "url": "https://github.com/tenex-engineering/graphql-x/issues"
  },
  "author": "Chabib N. R. <cnr@tenex.engineering> (https://tenex.engineering)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tenex-engineering/graphql-x.git"
  },
  "type": "module",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vitest",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "build": "tsc",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run"
  },
  "dependencies": {
    "graphql": "^16.9.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}
