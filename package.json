{
  "name": "@txe/graphql-x",
  "version": "0.13.0",
  "scripts": {
    "dev": "concurrently --names=\"build,test\" --prefix-colors=\"cyan,green\" \"npm run watch:build\" \"npm run watch:test\"",
    "ci": "concurrently --names=\"build,test,lint,format,spelling,exports\" \"npm run build\" \"npm run test\" \"npm run lint\" \"npm run check-format\" \"npm run check-spelling\" \"npm run check-exports\"",
    "build": "tsc --project tsconfig.build.json",
    "test": "vitest run --coverage",
    "test:unit": "vitest run --exclude=\"**/*.integration.spec.*\" --coverage",
    "test:integration": "vitest run .integration.spec.",
    "lint": "eslint ./",
    "check-format": "prettier --check .",
    "check-spelling": "cspell **",
    "check-exports": "attw --pack --ignore-rules=cjs-resolves-to-esm ./",
    "watch:build": "tsc --noEmit --watch --preserveWatchOutput",
    "watch:test": "vitest --exclude=\"**/*.integration.spec.*\" --coverage --clearScreen=false",
    "lint-fix": "eslint --fix ./",
    "format": "prettier --write ./"
  },
  "description": "GraphQL eXpansion",
  "license": "MIT",
  "author": "Chabib N. R. <cnr@tenex.engineering> (https://tenex.engineering)",
  "homepage": "https://github.com/tenex-engineering/graphql-x#readme",
  "bugs": {
    "url": "https://github.com/tenex-engineering/graphql-x/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tenex-engineering/graphql-x.git"
  },
  "keywords": [
    "graphql",
    "expansion"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "bin": {
    "graphql-x": "bin/graphql-x"
  },
  "files": [
    "./dist/"
  ],
  "imports": {
    "#app/*": "./dist/*"
  },
  "packageManager": "pnpm@9.12.1",
  "dependencies": {
    "@parcel/watcher": "^2.5.0",
    "debounce": "^2.2.0",
    "graphql": "^16.9.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.0",
    "@changesets/cli": "^2.27.9",
    "@cspell/cspell-types": "^8.16.0",
    "@eslint/compat": "^1.2.3",
    "@eslint/js": "^9.14.0",
    "@stylistic/eslint-plugin-js": "^2.10.1",
    "@txe/eslint-plugin-x": "^0.1.0",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "^2.1.5",
    "concurrently": "^9.1.0",
    "cspell": "^8.16.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.5"
  }
}
