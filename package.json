{
  "name": "@txe/graphql-x",
  "version": "0.1.5",
  "description": "GraphQL eXpansion",
  "keywords": [
    "graphql",
    "expansion"
  ],
  "homepage": "https://github.com/tenex-engineering/graphql-x#readme",
  "bugs": {
    "url": "https://github.com/tenex-engineering/graphql-x/issues"
  },
  "author": "Chabib N. R. <cnr@tenex.engineering> (https://tenex.engineering)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tenex-engineering/graphql-x.git"
  },
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.12.1",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "!**/*.test.*",
    "!**/*.js.map"
  ],
  "bin": {
    "graphql-x": "./bin/graphql-x"
  },
  "imports": {
    "#app/*": "./dist/*",
    "#documents/*": "./documents/*"
  },
  "scripts": {
    "prepublishOnly": "npm run ci",
    "dev": "concurrently --names=\"build,test\" --prefix-colors=\"cyan,green\" \"npm run watch:build -- --noEmit\" \"npm run watch:test\"",
    "ci": "npm run lint && npm run build && npm run check-format && npm run check-exports && npm run test",
    "local-release": "changeset version && changeset publish",
    "build": "tsc",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run",
    "watch:build": "tsc --watch --preserveWatchOutput",
    "watch:test": "vitest --clearScreen=false"
  },
  "dependencies": {
    "@parcel/watcher": "^2.4.1",
    "graphql": "^16.9.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "@eslint/compat": "^1.2.0",
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.7",
    "concurrently": "^9.0.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  }
}
